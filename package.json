{
  "name": "lambda-backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "npm run copy-env && tsc-watch --onSuccess \"node --stack-trace-limit=1000 --async-stack-traces ./build/server.js\"",
    "build": "tsc",
    "build:prod": "npm run build && npm run copy && npm run copy-env-production && npm run webpack && npm run copy-package-json && npm run copy-modules",
    "lint": "eslint . --ext .ts",
    "copy": "ncp package.json build/package.json",
    "copy-env": "ncp ./src/environments/.env ./build/.env",
    "copy-env-development": "ncp ./src/environments/.development.env ./build/.env",
    "copy-env-production": "ncp ./src/environments/.production.env ./build/.env",
    "copy-env-staging": "ncp ./src/environments/.staging.env ./build/.env",
    "clean": "rimraf build/* && rimraf build/.env",
    "webpack": "npx webpack",
    "copy-modules": "ncp ./node_modules ./dist/node_modules",
    "copy-package-json": "ncp ./package.json ./dist/package.json"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:autonomous-ecm/blockchain/lambda-backend.git"
  },
  "keywords": [],
  "author": "Nhan Nguyen",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/forwarded": "^0.1.0",
    "@types/node": "^17.0.7",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "ncp": "^2.0.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-bearer-token": "^2.4.0",
    "express-rate-limit": "^6.0.4",
    "forwarded": "^0.2.0",
    "helmet": "^5.0.1",
    "http-status-codes": "^2.2.0",
    "mysql": "^2.18.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  }
}
